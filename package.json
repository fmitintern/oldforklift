{
  "name": "forklift-tracking",
  "version": "1.0.0",
  "type": "module",
  "description": "Forklift Booking & Tracking Progressive Web App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "nodemon server.js",
    "dev-client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase": "^11.4.0",
    "mongoose": "^7.8.6",
    "oracledb": "^6.8.0",
    "react-router-dom": "^6.4.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
